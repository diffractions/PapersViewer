<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">


	<bean id="paperDao" class="dao.impl.PaperDaoDemo"
		scope="singleton">
		<property name="papers">
			<map key-type="java.lang.Integer" value-type="entity.Paper">
				<entry key="1" value-ref="Paper1"></entry>
				<entry key="2" value-ref="Paper2"></entry>
				<entry key="3" value-ref="Paper3"></entry>
				<entry key="4" value-ref="Paper4"></entry>
				<entry key="5" value-ref="Paper5"></entry>
			</map>
		</property>
	</bean>

	<bean id="Paper1" class="entity.Paper" scope="singleton">
		<constructor-arg value="1"></constructor-arg>
		<constructor-arg type="java.lang.String" value="Hello"></constructor-arg>
	</bean>

	<bean id="Paper2" class="entity.Paper" scope="singleton">
		<constructor-arg value="2" ></constructor-arg>
		<constructor-arg value="you" type="java.lang.String"></constructor-arg>
	</bean>

	<bean id="Paper3" class="entity.Paper" scope="singleton">
		<constructor-arg value="3" ></constructor-arg>
		<constructor-arg value="get" type="java.lang.String"></constructor-arg>
	</bean>

	<bean id="Paper4" class="entity.Paper" scope="singleton">
		<constructor-arg value="4" ></constructor-arg>
		<constructor-arg value="new" type="java.lang.String"></constructor-arg>
	</bean>

	<bean id="Paper5" class="entity.Paper" scope="singleton">
		<constructor-arg value="5" ></constructor-arg>
		<constructor-arg value="paper" type="java.lang.String"></constructor-arg>
	</bean>
</beans>
